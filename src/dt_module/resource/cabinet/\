<?xml version="1.0"?>
<launch>
  <param name="cabinet" textfile="$(find dt_module)/resource/cabinet/model.sdf" />
  <param name="robot_collid" textfile="$(find dt_module)/resource/robot_collid/model.sdf" />
  <param name="jar" textfile="$(find dt_module)/resource/jar/model.sdf" />

  <node name="spawn_cabinet0" pkg="gazebo_ros" type="spawn_model" args="-param cabinet -sdf -model cabinet0  -y 2.75 -Y -1.570796326794897" />
  <node name="spawn_cabinet1" pkg="gazebo_ros" type="spawn_model" args="-param cabinet -sdf -model cabinet1 -x -1.76 -y 2.75 -Y -1.570796326794897" />
  <node name="spawn_cabinet2" pkg="gazebo_ros" type="spawn_model" args="-param cabinet -sdf -model cabinet2 -x -0.88 -y 2.75 -Y -1.570796326794897" />
  <node name="spawn_cabinet3" pkg="gazebo_ros" type="spawn_model" args="-param cabinet -sdf -model cabinet3 -x 0.88  -y 2.75 -Y -1.570796326794897" />
  <node name="spawn_cabinet4" pkg="gazebo_ros" type="spawn_model" args="-param cabinet -sdf -model cabinet4 -x 1.76 -y 2.75 -Y -1.570796326794897" />
  <node name="spawn_cabinet5" pkg="gazebo_ros" type="spawn_model" args="-param cabinet -sdf -model cabinet5 -x 2.64 -y 2.75 -Y -1.570796326794897" />
  <node name="spawn_robot_collid" pkg="gazebo_ros" type="spawn_model" args="-param robot_collid -sdf -model robot_collid" />

  <node name="spawn_jar0" pkg="gazebo_ros" type="spawn_model" args="-param jar -sdf -model jar0 -x 0.0 -y 2.7 -z 0.965" />
  <node name="spawn_jar1" pkg="gazebo_ros" type="spawn_model" args="-param jar -sdf -model jar1 -x 0.1 -y 2.7 -z 0.965" />
  <node name="spawn_jar2" pkg="gazebo_ros" type="spawn_model" args="-param jar -sdf -model jar2 -x 0.2 -y 2.7 -z 0.965" />
  <node name="spawn_jar3" pkg="gazebo_ros" type="spawn_model" args="-param jar -sdf -model jar3 -x -0.1 -y 2.7 -z 0.965" />
  <node name="spawn_jar4" pkg="gazebo_ros" type="spawn_model" args="-param jar -sdf -model jar4 -x -0.2 -y 2.7 -z 0.965" />
</launch>
