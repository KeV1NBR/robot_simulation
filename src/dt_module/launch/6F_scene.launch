<?xml version="1.0"?>
<launch>
  <param name="cabinet" textfile="$(find dt_module)/resource/cabinet/model.sdf" />
  <param name="robot_collid" textfile="$(find dt_module)/resource/robot_collid/model.sdf" />
  <param name="jar" textfile="$(find dt_module)/resource/jar/model.sdf" />
  <param name="wall" textfile="$(find dt_module)/resource/wall/model.sdf" />
  <param name="meeting_table" textfile="$(find dt_module)/resource/meeting_table/model.sdf" />
  <param name="office_desk" textfile="$(find dt_module)/resource/office_desk/model.sdf" />
  <param name="kinect" textfile="$(find dt_module)/resource/kinect/model.sdf" />

  <node name="spawn_cabinet0" pkg="gazebo_ros" type="spawn_model" args="-param cabinet -sdf -model cabinet0  -y 2.795 -Y -1.570796326794897" />
  <node name="spawn_cabinet1" pkg="gazebo_ros" type="spawn_model" args="-param cabinet -sdf -model cabinet1 -x -1.76 -y 2.795 -Y -1.570796326794897" />
  <node name="spawn_cabinet2" pkg="gazebo_ros" type="spawn_model" args="-param cabinet -sdf -model cabinet2 -x -0.88 -y 2.795 -Y -1.570796326794897" />
  <node name="spawn_cabinet3" pkg="gazebo_ros" type="spawn_model" args="-param cabinet -sdf -model cabinet3 -x 0.88 -y 2.795 -Y -1.570796326794897" />
  <node name="spawn_cabinet4" pkg="gazebo_ros" type="spawn_model" args="-param cabinet -sdf -model cabinet4 -x 1.76 -y 2.795 -Y -1.570796326794897" />
  <node name="spawn_cabinet5" pkg="gazebo_ros" type="spawn_model" args="-param cabinet -sdf -model cabinet5 -x 2.64 -y 2.795 -Y -1.570796326794897" />
  <node name="spawn_meeting_table" pkg="gazebo_ros" type="spawn_model" args="-param meeting_table -sdf -model meeting_table -x 2.66 -y 1.665" />
  <node name="spawn_office_desk0" pkg="gazebo_ros" type="spawn_model" args="-param office_desk -sdf -model office_desk0 -x 0.02 -y 0.005 -Y 3.1415926" />
  <node name="spawn_office_desk1" pkg="gazebo_ros" type="spawn_model" args="-param office_desk -sdf -model office_desk1 -x -1.43 -y 0.005 -Y 3.1415926" />
  <node name="spawn_office_desk2" pkg="gazebo_ros" type="spawn_model" args="-param office_desk -sdf -model office_desk2 -x -2.89 -y 0.005 -Y 3.1415926" />
  <node name="spawn_kinect" pkg="gazebo_ros" type="spawn_model" args="-param kinect -sdf -model kinect -z 1.5 -Y 1.570796326794897" />
  <node name="spawn_wall" pkg="gazebo_ros" type="spawn_model" args="-param wall -sdf -model wall -x 0.39 -y 1.32 -Y -1.570796326794897" />


  <node name="spawn_robot_collid" pkg="gazebo_ros" type="spawn_model" args="-param robot_collid -sdf -model robot_collid -x -99" />

  <node name="spawn_jar0" pkg="gazebo_ros" type="spawn_model" args="-param jar -sdf -model jar0 -x -99 -y 2.7 -z 0.965" />
  <node name="spawn_jar1" pkg="gazebo_ros" type="spawn_model" args="-param jar -sdf -model jar1 -x -99 -y 2.7 -z 0.965" />
  <node name="spawn_jar2" pkg="gazebo_ros" type="spawn_model" args="-param jar -sdf -model jar2 -x -99 -y 2.7 -z 0.965" />
  <node name="spawn_jar3" pkg="gazebo_ros" type="spawn_model" args="-param jar -sdf -model jar3 -x -99 -y 2.7 -z 0.965" />
  <node name="spawn_jar4" pkg="gazebo_ros" type="spawn_model" args="-param jar -sdf -model jar4 -x -99 -y 2.7 -z 0.965" />
